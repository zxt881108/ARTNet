name: "C3D-t1"
mem_param {
  optimize_test: true
  optimize_train: true
}
########################################### data #############################################
layer {
  name: "data"
  type: "VideoData"
  top: "data"
  top: "label"
  video_data_param {
    source: "train_rgb_final.txt"
    batch_size: 8
    new_width: 340
    new_height: 256
    new_length: 32
    num_segments: 1
    modality: RGB
    length_first: true
    name_pattern: "image_%05d.jpg"
    shuffle: true
  }
  transform_param {
    crop_size: 224
    mirror: true
    multi_scale: true
    max_distort: 1
    scale_ratios: [1, 0.875, 0.75, 0.66]
    mean_value: [104, 104, 104, 104, 104, 104, 104, 104, 104, 104, 104, 104, 104, 104, 104, 104]
    mean_value: [117, 117, 117, 117, 117, 117, 117, 117, 117, 117, 117, 117, 117, 117, 117, 117]
    mean_value: [123, 123, 123, 123, 123, 123, 123, 123, 123, 123, 123, 123, 123, 123, 123, 123]
  }
  include: { phase: TRAIN }
}
layer {
  name: "data"
  type: "VideoData"
  top: "data"
  top: "label"
  video_data_param {
    source: "val_rgb_final.txt"
    batch_size: 1
    new_length: 32
    new_width: 240
    new_height: 256
    num_segments: 1
    modality: RGB
    name_pattern: "image_%05d.jpg"
    length_first: true
  }
  transform_param {
    crop_size: 224
    mirror: false
    mean_value: [104, 104, 104, 104, 104, 104, 104, 104, 104, 104, 104, 104, 104, 104, 104, 104]
    mean_value: [117, 117, 117, 117, 117, 117, 117, 117, 117, 117, 117, 117, 117, 117, 117, 117]
    mean_value: [123, 123, 123, 123, 123, 123, 123, 123, 123, 123, 123, 123, 123, 123, 123, 123]
  }
  include: { phase: TEST }
}
layer {
  name: "data_reshape"
  type: "Reshape"
  bottom: "data"
  top: "data_reshape"
  reshape_param{
    shape { dim: -1 dim: 3 dim: 32 dim: 224 dim: 224 } 
  }
}

############################################# energy1 ########################################
layer {
  name: "conv1"
  type: "Convolution"
  bottom: "data_reshape"
  top: "conv1"
  convolution_param {
    num_output: 64
    pad: [0, 3, 3]
    kernel_size: [1, 7, 7]
    stride: [2, 2, 2]
    weight_filler{
       type: "xavier"
    }
    bias_term: false
  }
  param { lr_mult: 1 decay_mult: 1 }
}
layer { 
  name: "conv1_bn" type: "BN" bottom: "conv1" top: "conv1_bn"
  param { lr_mult: 1 decay_mult: 0 } param { lr_mult: 1 decay_mult: 0 }
  bn_param { slope_filler { type: "constant" value: 1 } bias_filler { type: "constant" value: 0 } } 
}
layer {
  name: "conv1_t"
  type: "Convolution"
  bottom: "data_reshape"
  top: "conv1_t"
  convolution_param {
    num_output: 64
    pad: [1, 3, 3]
    kernel_size: [3, 7, 7]
    stride: [2, 2, 2]
    weight_filler{
       type: "xavier"
    }
    bias_term: false
  }
  param { lr_mult: 1 decay_mult: 1 }
}
layer { 
  name: "conv1_t_bn" type: "BN" bottom: "conv1_t" top: "conv1_t_bn"
  param { lr_mult: 1 decay_mult: 0 } param { lr_mult: 1 decay_mult: 0 }
  bn_param { slope_filler { type: "constant" value: 1 } bias_filler { type: "constant" value: 0 } } 
}
layer {
  name: "energy1"
  bottom: "conv1_t_bn"
  top: "energy1"
  type: "Power"
  power_param { power: 2 scale: 1 shift: 0 }
}
layer {
  name: "pooling1"
  bottom: "energy1"
  top: "pooling1"
  type: "Convolution"
  convolution_param {
    num_output: 32
    pad: [0, 0, 0]
    kernel_size: [1, 1, 1]
    stride: [1, 1, 1]
    weight_filler{
       type: "constant"
       value: 0.5
    }
    bias_term: false
    group: 32
  }
  param { lr_mult: 0 decay_mult: 0 }
}
layer {
  name: "pooling1_bn"
  bottom: "pooling1"
  top: "pooling1_bn"
  type: "BN"
  param { lr_mult: 1 decay_mult: 0 } param { lr_mult: 1 decay_mult: 0 }
  bn_param { slope_filler { type: "constant" value: 1 } bias_filler { type: "constant" value: 0 } } 
}
layer {
  name: "concat1"
  bottom: "conv1_bn"
  bottom: "pooling1_bn"
  top: "concat1"
  type: "Concat"
}
layer {
  name: "concat1_relu"
  bottom: "concat1"
  top: "concat1"
  type: "ReLU"
}
layer {
  name: "redu1"
  bottom: "concat1"
  top: "redu1"
  type: "Convolution"
  convolution_param {
    num_output: 64
    pad: [0, 0, 0]
    kernel_size: [1, 1, 1]
    stride: [1, 1, 1]
    weight_filler{
       type: "xavier"
    }
    bias_term: false
  }
  param { lr_mult: 1 decay_mult: 1 }
}
layer {
  name: "redu1_bn"
  bottom: "redu1"
  top: "redu1_bn"
  type: "BN"
  param { lr_mult: 1 decay_mult: 0 } param { lr_mult: 1 decay_mult: 0 }
  bn_param { slope_filler { type: "constant" value: 1 } bias_filler { type: "constant" value: 0 } } 
}
layer {
  name: "redu1_relu"
  bottom: "redu1_bn"
  top: "redu1_bn"
  type: "ReLU"
}
############################################## pool-extra ##########################################
layer {
  name: "pooling1-extra"
  bottom: "redu1_bn"
  top: "pooling1-extra"
  type: "Convolution"
  convolution_param {
    num_output: 64
    pad: [1, 1, 1]
    kernel_size: [3, 3, 3]
    stride: [2, 2, 2]
    weight_filler{
       type: "constant"
       value: 0
    }
    bias_term: false
    group: 64
  }
  param { lr_mult: 0 decay_mult: 0 }
}
layer {
  name: "pooling1-extra_bn"
  bottom: "pooling1-extra"
  top: "pooling1-extra_bn"
